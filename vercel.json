{
    "version": 2,
    "buildCommand": "exit 0",
    "outputDirectory": "./",
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/(?!public/|index\\.html|vercel\\.json)(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/public/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                }
            ]
        }
    ]
}